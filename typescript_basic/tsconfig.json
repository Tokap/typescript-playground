{
    "compilerOptions": {
        "target": "es6",
        "jsx": "react",
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "rootDir": "src",
        "sourceMap": true,
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true
    },
    "include": [
        "./src"
    ],
    "exclude": [
        "node_modules"
    ]
}