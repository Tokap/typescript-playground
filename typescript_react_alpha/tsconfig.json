{
    "compilerOptions": {
        "jsx": "react",
        "module": "system",
        "noImplicitAny": true,
        "outFile": "./src/compiled/translated-scripts.js",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es6"
    },
    "include": [
        "src/*"
    ],
    "exclude": [
        "node_modules"
    ]
}